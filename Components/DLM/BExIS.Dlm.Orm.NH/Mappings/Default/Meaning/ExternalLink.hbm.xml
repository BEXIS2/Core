<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Dlm.Entities" namespace="BExIS.Dlm.Entities.Meanings">

	<class xmlns="urn:nhibernate-mapping-2.2" name="ExternalLink" table="ExternalLink">

    <id name="Id" type="Int64">
      <generator class="native" />
    </id>

    <version name="VersionNo" type="Int32">
      <column name="VersionNo" />
    </version>

    <property name="URI" type="String">
      <column name="URI" />
    </property>
    
    <property name="Name" type="String">
      <column name="Name" />
    </property>
    
    <property name="Type" type="String">
      <column name="Type" />
    </property>
<!--
	<many-to-one name="Meaning" class="Meaning" not-null="false" column="externalLinkref"/>
-->

	</class>
</hibernate-mapping>
