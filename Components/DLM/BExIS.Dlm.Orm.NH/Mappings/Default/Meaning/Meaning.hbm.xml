<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Dlm.Entities" namespace="BExIS.Dlm.Entities.Meanings">

  <class xmlns="urn:nhibernate-mapping-2.2" name="Meaning" table="Meanings" dynamic-update="true">

    <id name="Id" type="Int64">
      <generator class="native" />
    </id>

    <version name="VersionNo" type="Int32">
      <column name="VersionNo" />
    </version>

	<property name="Name" type="String">
	 <column name="Name" />
	</property>

	<property name="ShortName" type="String">
	 <column name="ShortName" />
	</property>

	<property name="Description" type="String">
	 <column name="Description" />
	</property>

	<property name="approved" type="BExIS.Dlm.Entities.Meanings.Approved, BExIS.Dlm.Entities">
	 <column name="approved"/>
	</property>

	<property name="selectable" type="BExIS.Dlm.Entities.Meanings.Selectable, BExIS.Dlm.Entities">
	 <column name="selectable"/>
	</property>

	  <bag  name="externalLink" inverse="false" lazy="true" fetch="select" >
		  <key>
			  <column name="externalLinkref" />
		  </key>
		  <one-to-many class="ExternalLink" />
	  </bag >


	  <set name="related_meaning" table="meaning_meaning" inverse="false" lazy="true" fetch="select" cascade="save-update" >
		  <key column="MeaningsParentRef"/>
		  <many-to-many column="MeaningsChildrenRef" class="Meaning"/>
	  </set>

	  <set name="variable" table="meanings_Variables" inverse="false" lazy="false" fetch="select" cascade="save-update"  >
		  <key column="id"/>
		  <many-to-many class="BExIS.Dlm.Entities.DataStructure.Variable" column="Variable"/>
	  </set>
	  
	  
	  <!--
		<set name="variable" table="meanings_Variables" inverse="false" lazy="true" fetch="select" cascade="none" >
		  <key column="id"/>
		  <many-to-many column="MeaningsParentRef" class="Variable"/>
	  </set>
	  -->
	  
  </class>
	
</hibernate-mapping>
