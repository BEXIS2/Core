<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Dlm.Entities" namespace="BExIS.Dlm.Entities.Meanings">

  <class xmlns="urn:nhibernate-mapping-2.2" name="Meaning" table="rpm_Meaning" dynamic-update="true">

    <id name="Id" type="Int64">
      <generator class="native" />
    </id>

    <version name="VersionNo" type="Int32">
      <column name="VersionNo" />
    </version>

	<property name="Name" type="String">
	 <column name="Name" />
	</property>

	<property name="ShortName" type="String">
	 <column name="ShortName" />
	</property>

	<property name="Description" type="String">
	 <column name="Description" />
	</property>

	<property name="Approved" type="BExIS.Dlm.Entities.Meanings.Approved, BExIS.Dlm.Entities">
	 <column name="Approved"/>
	</property>

	<property name="Selectable" type="BExIS.Dlm.Entities.Meanings.Selectable, BExIS.Dlm.Entities">
	 <column name="Selectable"/>
	</property>

	  <list name="Related_meaning" table="rpm_meaning_meaning" inverse="false" lazy="false" fetch="select" cascade="save-update" >
		  <key column="MeaningParentRef"></key>
		  <index column="id"></index>
		  <many-to-many class="BExIS.Dlm.Entities.Meanings.Meaning" column="MeaningchildrenRef"/>
	  </list>
	  
	  <list name="ExternalLinks" table="rpm_meanings_MeaningEntry" inverse="false" lazy="false" fetch="select" cascade="save-update" >
		  <key column="MeaningRef"></key>
		  <index column="id"></index>
		  <many-to-many class="BExIS.Dlm.Entities.Meanings.MeaningEntry" column="MeaningEntryRef"/>
	  </list>


  </class>
	
</hibernate-mapping>
