<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Dlm.Entities" namespace="BExIS.Dlm.Entities.Meanings">

  <class xmlns="urn:nhibernate-mapping-2.2" name="Meaning" table="rpm_meanings" dynamic-update="true">

    <id name="Id" type="Int64">
      <generator class="native" />
    </id>

    <version name="VersionNo" type="Int32">
      <column name="VersionNo" />
    </version>

	<property name="Name" type="String">
	 <column name="Name" />
	</property>

	<property name="ShortName" type="String">
	 <column name="ShortName" />
	</property>

	<property name="Description" type="String">
	 <column name="Description" />
	</property>

	<property name="Approved" type="BExIS.Dlm.Entities.Meanings.Approved, BExIS.Dlm.Entities">
	 <column name="Approved"/>
	</property>

	<property name="Selectable" type="BExIS.Dlm.Entities.Meanings.Selectable, BExIS.Dlm.Entities">
	 <column name="Selectable"/>
	</property>

	  <set name="ExternalLink" table="rpm_meaning_externallink" inverse="false" lazy="true" fetch="select" cascade="save-update" >
		  <key column="MeaningsRef"/>
		  <many-to-many column="ExternalLinkref" class="ExternalLink"/>
	  </set>


	  <set name="Related_meaning" table="rpm_meaning_meaning" inverse="false" lazy="true" fetch="select" cascade="save-update" >
		  <key column="MeaningsParentRef"/>
		  <many-to-many column="MeaningsChildrenRef" class="Meaning"/>
	  </set>

	  
  </class>
	
</hibernate-mapping>
