<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Dlm.Entities" namespace="BExIS.Dlm.Entities.DataStructure">
  <subclass name="VariableInstance" extends="Variable" discriminator-value="VAR_INST" dynamic-update="true">

	  <property name="OrderNo" type="Int32">
		  <column name="OrderNo" />
	  </property>

	  <property name="DisplayPatternId" type="Int32">
		  <column name="DisplayPatternId" />
	  </property>

	  <property name="IsKey" type="Boolean">
		  <column name="IsKey" />
	  </property>

	  <many-to-one name="DataStructure" class="StructuredDataStructure" not-null="false" column="DataStructureRef" index="idx_DataStructureRef_Variables" />
	  <many-to-one name="VariableTemplate" class="Variable" not-null="false" column="VarTemplateRef"/>

	  <set name="MissingValues" inverse="true">
		  <key column="VariableRef" not-null="false" />
		  <one-to-many class="MissingValue" />
	  </set>
  </subclass>
</hibernate-mapping>
