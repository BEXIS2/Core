{
	"name": "bexis.web.shell.svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build ",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"postinstall": "patch-package",
		"create patch": "npx patch-package @sveltejs/kit",
		"install local bexis2-core-ui": "npm install C:/Projects/github.com/BEXIS2/bexis2-core-ui",
		"reinstall packages": "npm install --force",
		"check packages": "npm outdated",
		"upgrade all": "ncu --upgrade"
	},
	"devDependencies": {
		"@codemirror/lang-html": "6.4.9",
		"@codemirror/lang-javascript": "6.2.2",
		"@codemirror/lang-json": "6.0.1",
		"@codemirror/theme-one-dark": "6.1.2",
		"@playwright/test": "1.45.3",
		"@skeletonlabs/skeleton": "2.10.2",
		"@skeletonlabs/tw-plugin": "0.4.0",
		"@sveltejs/adapter-auto": "3.2.2",
		"@sveltejs/kit": "2.5.19",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.13",
		"@types/node": "22.0.2",
		"@typescript-eslint/eslint-plugin": "8.0.0",
		"@typescript-eslint/parser": "8.0.0",
		"autoprefixer": "10.4.19",
		"codemirror": "6.0.1",
		"eslint": "9.8.0",
		"eslint-config-prettier": "9.1.0",
		"postcss": "8.4.40",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.6",
		"svelte": "4.2.18",
		"svelte-check": "3.8.5",
		"svelte-codemirror-editor": "1.4.0",
		"tailwindcss": "3.4.7",
		"tslib": "2.6.3",
		"typescript": "5.5.4",
		"vite": "5.3.5",
		"vitest": "2.0.5"
	},
	"type": "module",
	"dependencies": {

		"@bexis2/bexis2-core-ui": "0.4.22",
		"@sveltejs/adapter-static": "3.0.2",
		"patch-package": "8.0.0",
		"svelte-collapsible": "0.3.0",
		"svelte-fa": "4.0.2"

	}
}
