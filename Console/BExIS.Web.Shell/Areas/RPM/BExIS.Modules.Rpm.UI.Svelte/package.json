{
  "name": "@bexis2/bexis2-rpm-ui",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build ",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "postinstall": "patch-package",
    "build package": "svelte-kit sync && svelte-package --watch",
    "check outdated": "npm outdated",
    "upgrade all": "ncu --upgrade",
    "1.npm - init": "npm init --scope bexis2",
    "2.npm - package": "svelte-package",
    "3.npm - publish": "npm publish --access public"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@skeletonlabs/skeleton": "^2.9.0",
    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.26",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "svelte-headless-table": "^0.18.2",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  },
  "type": "module",
  "dependencies": {
    "@bexis2/bexis2-core-ui": "0.4.0",
    "@floating-ui/dom": "^1.6.3",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/package": "^2.3.0",
    "papaparse": "^5.4.1",
    "patch-package": "^8.0.0",
    "svelte-fa": "^4.0.2"
  },
  "author": "David Sch√∂ne",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BEXIS2/Core.git"
  },
  "bugs": {
    "url": "https://github.com/BEXIS2/Core/issues"
  },
  "homepage": "https://github.com/BEXIS2/Core#readme",
  "keywords": [
    "bexis2",
    "libary"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "module": "./src/lib/index.ts",
  "types": "./src/lib/index.d.ts",
  "description": "Everything you need to build a Svelte project, powered by [`create-svelte`](https://github.com/sveltejs/kit/tree/master/packages/create-svelte).",
  "main": "./src/lib/index.d.ts",
  "files": [
    "dist",
    "src/lib"
  ],
  "directories": {
    "test": "tests"
  }
}
