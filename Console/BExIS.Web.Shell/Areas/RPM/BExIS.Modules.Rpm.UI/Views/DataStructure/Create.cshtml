@{
    ViewBag.Title = "Structure Suggestion";
    Layout = "~/Themes/Default/Layouts/_svelteLayout.cshtml";

}

@{
    long id = 0;
    long version = 0;
    string file = "";
    long structureId = 0;

    if (ViewData["id"] != null) { id = Convert.ToInt64(ViewData["id"]); }

    if (ViewData["version"] != null) { version = Convert.ToInt64(ViewData["version"]); }

    if (ViewData["file"] != null) { file = ViewData["file"].ToString(); }

    if (ViewData["structureId"] != null) { structureId = Convert.ToInt64(ViewData["structureId"]); }

}

<div id="datastructure" dataset="@id" version="@version" file="@file" structure="@structureId">
    @Html.Partial("_sveltePage")
</div>