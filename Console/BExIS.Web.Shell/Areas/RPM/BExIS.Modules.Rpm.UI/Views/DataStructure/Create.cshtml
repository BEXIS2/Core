@{
    ViewBag.Title = "Structure Suggestion";
    Layout = "~/Themes/Default/Layouts/_svelteLayout.cshtml";

}

@{
    long id = 0;
    long version = 0;
    string file = "";
    long structureId = 0;

    string isTemplateRequired = "false";
    string isMeaningRequired = "false";
    string setByTemplate = "false";
    string enforcePrimaryKey = "false";
    string changeablePrimaryKey = "true";
    string showDarwinCoreValidation = "false";

    if (ViewData["id"] != null) { id = Convert.ToInt64(ViewData["id"]); }
    if (ViewData["version"] != null) { version = Convert.ToInt64(ViewData["version"]); }
    if (ViewData["file"] != null) { file = ViewData["file"].ToString(); }
    if (ViewData["structureId"] != null) { structureId = Convert.ToInt64(ViewData["structureId"]); }
    if (ViewData["isTemplateRequired"] != null) { isTemplateRequired = ViewData["isTemplateRequired"].ToString(); }
    if (ViewData["setByTemplate"] != null) { setByTemplate = ViewData["setByTemplate"].ToString(); }
    if (ViewData["enforcePrimaryKey"] != null) { enforcePrimaryKey = ViewData["enforcePrimaryKey"].ToString(); }
    if (ViewData["showDarwinCoreValidation"] != null) { showDarwinCoreValidation = ViewData["showDarwinCoreValidation"].ToString(); }
    //if (ViewData["changeablePrimaryKey"] != null) { changeablePrimaryKey = ViewData["changeablePrimaryKey"].ToString(); }

}
<div id="datastructure" structure="@structureId" dataset="@id" file="@file" isTemplateRequired="@isTemplateRequired" isMeaningRequired="@isMeaningRequired" setByTemplate="@setByTemplate" enforcePrimaryKey="@enforcePrimaryKey" changeablePrimaryKey="@changeablePrimaryKey" showDarwinCoreValidation="@showDarwinCoreValidation">
    @Html.Partial("_sveltePage")
</div>