		"svelte": "4.2.12",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.4",
		"vite": "^5.2.8",
		"vitest": "^1.4.0"
	},
	"type": "module",
	"dependencies": {
		"@bexis2/bexis2-core-ui": "0.4.5",
