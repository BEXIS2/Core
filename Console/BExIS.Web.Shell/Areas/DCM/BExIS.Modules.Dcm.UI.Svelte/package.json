{
	"name": "@bexis2/bexis2-dcm-ui",
	"version": "0.0.1",
	"private": false,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build watch": "vite build --watch",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
