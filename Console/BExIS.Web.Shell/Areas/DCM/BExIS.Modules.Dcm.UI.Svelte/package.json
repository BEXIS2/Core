		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/package": "^2.3.1",
		"patch-package": "^8.0.0",
		"svelte-adapter-github": "^1.0.0-next.0",
		"svelte-fa": "^4.0.2",
		"svelte-headless-table": "^0.18.2",
		"svelte-select": "^5.8.3",
		"vest": "^5.2.12"
