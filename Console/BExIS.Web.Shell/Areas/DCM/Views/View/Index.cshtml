@{
    ViewBag.Title = "View";
    Layout = "~/Themes/Default/Layouts/_svelte_telerikLayout.cshtml";

}

<link href="~/Areas/Dcm/Scripts/svelte/view.css" rel="stylesheet">


@section scripts
{
    <script src="~/Areas/Dcm/Scripts/svelte/view.js"></script>

}

@{
    long id = 0;
    long version = 0;

    if (ViewBag.id != null)
    {
        id = (long)ViewBag.id;
    }

    if (ViewBag.version != null)
    {
        version = (long)ViewBag.version;
    }

}

<div id="view" dataset="@id" version="@version"> </div>
