		<add key="TenantId" value="bexis" />
		<add key="ThemesPath" value="~/Themes/" />
		<add key="DefaultThemeName" value="default" />
		<add key="ActiveLayoutName" value="_Layout" />

		<add key="loginUrl" value="Account/Login" />
