		<add key="IsCallLoggingEnable" value="true" />
		<add key="IsPerformanceLoggingEnable" value="true" />
		<add key="IsDiagnosticLoggingEnable" value="true" />
		<add key="IsExceptionLoggingEnable" value="true" />
