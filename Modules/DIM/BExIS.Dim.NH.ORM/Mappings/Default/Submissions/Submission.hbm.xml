<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Dim.Entities" namespace="BExIS.Dim.Entities.Submissions">
	<class xmlns="urn:nhibernate-mapping-2.2" name="Submission" table="DIM_Submissions">
		<!-- Mapping BaseEntity properties -->

		<id name="Id" type="Int64">
			<column name="Id" />
			<generator class="native" />
		</id>

		<version name="VersionNo" type="Int32">
			<column name="VersionNo" />
		</version>

		<property name="Extra" type="System.Xml.XmlDocument, System.Xml, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<column name="Extra" not-null="false" sql-type="xml" />
		</property>

		<!-- Mapping Publication properties -->

		<property name="Status" type="BExIS.Dim.Entities.Submissions.SubmissionStatus, BExIS.Dim.Entities">
			<column name="Status" />
		</property>

		<property name="CreationDate" type="DateTime">
			<column name="CreationDate" />
		</property>

		<property name="LastModificationDate" type="DateTime">
			<column name="LastModificationDate" />
		</property>

		<property name="Content" type="StringClob">
			<column name="Content" />
		</property>

		<many-to-one name="DatasetVersion" column="DatasetVersionRef" class="BExIS.Dlm.Entities.Data.DatasetVersion, BExIS.Dlm.Entities" />

		<many-to-one name="Agent" column="AgentRef" class="BExIS.Dim.Entities.Submissions.Agent, BExIS.Dim.Entities" />
	</class>
</hibernate-mapping>